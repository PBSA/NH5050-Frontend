{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": "off",
      "react/prop-types": "off",
      "react/prefer-stateless-function": "off",
      "react/destructuring-assignment": "off",
      "react/jsx-filename-extension": "off"
    },
    "ignorePatterns": ["dist/", "node_modules", "webpack.config.js", "scripts"]
}